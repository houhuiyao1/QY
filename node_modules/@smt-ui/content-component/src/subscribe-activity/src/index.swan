<!-- 预约组件页面 -->
<view class="subscribe">
    <form report-submit="{{true}}" report-type="subscribe" template-id="{{templateId}}" subscribe-id="{{subscribeId}}" bindsubmit="addSubscribe" bindreset="formReset">
        <view class="add-subscribe-wrap {{isLoading ? 'add-subscribe-no-text' : ''}}">
            <button s-if="isSubscribed" class="add-subscribe btn-subscribe-selected {{btnBgClass}} subscribed-class" type="primary" loading="{{isLoading}}" bindtap="cancelSubscribe">
                {{statusText[1] || subscribedText}}
            </button>
            <button s-else class="add-subscribe {{btnBgClass}} unsubscribed-class" type="primary" loading="{{isLoading}}" formType="submit">
                {{statusText[0] || unsubscribedText}}
            </button>
        </view>
    </form>
</view>
